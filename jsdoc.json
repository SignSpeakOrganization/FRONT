{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "include": ["app/scripts"],
    "includePattern": ".js$",
    "excludePattern": "(tests|node_modules)"
  },
  "plugins": [],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "opts": {
    "destination": "./docs/jsdoc",
    "recurse": true
  }
}
